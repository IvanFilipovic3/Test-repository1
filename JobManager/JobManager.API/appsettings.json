{
	"Logging": {
		"LogConfigFile": "log4net.config"
	},
	"CommonPlugins": [ "FIS.Risk.Core.Logging.Log4Net" ],
	"WebHostConfig": {
		"Rest": {
			"urls": "http://*:5000",
			"AllowedHosts": "*",
			"Logging": {
				"LogLevel": {
					"Default": "Information",
					"System": "Warning",
					"Microsoft": "Warning"
				}
			}
		}
	}
}
